{
  "name": "@appdirect/sfb-toolkit",
  "version": "4.0.7",
  "description": "",
  "main": "index.js",
  "bin": {
    "sfb-toolkit": "./bin/index.js"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./src/tests/__mocks__/setupTests.js"
    ],
    "setupFiles": [
      "./src/tests/__mocks__/localstorage.js"
    ],
    "testEnvironmentOptions": {
      "url": "http://localhost"
    },
    "testMatch": [
      "**/src/tests/**/*test.js?(x)"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(react-syntax-highlighter)/)"
    ],
    "globals": {
      "window": {}
    }
  },
  "license": "SEE LICENSE IN https://help.appdirect.com/sfb-eula.pdf",
  "scripts": {
    "test": "jest --coverage",
    "buildToolbar": "webpack --config ./webpack.config.cjs --mode production",
    "dev": "RUN_ENV=dev concurrently \"npm run watchCSS\" \"npm run devserver\"",
    "start": "RUN_ENV=test concurrently \"npm run watchCSS\" \"npm run devserver\"",
    "devserver": "nodemon ./index.js",
    "prepare": "npm run buildToolbar",
    "testTheme": "node src/tools/toolkit-tests.js",
    "testThemeWithMocks": "MOCK=true node src/tools/toolkit-tests.js",
    "mock": "node src/tools/toolkit-mock.js",
    "format": "prettier --write \"./src/**/*.js\"",
    "lint": "eslint src tools --ignore-path .eslintignore",
    "lintfix": "eslint src tools --ignore-path .eslintignore --fix",
    "watchCSS": "RUNDEVSERVER=true node src/tools/compiler-css.js",
    "package": "node src/tools/compiler-css.js && node src/tools/package-fonts.js && node src/tools/zip-theme.js",
    "docker:toolkit:build": "DOCKER_TAG=${DOCKER_TAG:=} docker build -t $npm_package_docker_registry/$npm_package_docker_name:$DOCKER_TAG .",
    "docker:toolkit:push": "DOCKER_TAG=${DOCKER_TAG:=} docker push $npm_package_docker_registry/$npm_package_docker_name:$DOCKER_TAG"
  },
  "author": "AppDirect, Inc.",
  "docker": {
    "registry": "docker.appdirect.tools",
    "name": "appdirect/sfb-toolkit"
  },
  "dependencies": {
    "@appdirect/public-custom-app-ui": "^0.0.17",
    "@appdirect/sfb-theme-classic": "0.0.26",
    "@appdirect/sfb-theme-plaza": "0.0.73",
    "@appdirect/storefront-utils": "latest",
    "@babel/core": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.19.0",
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "app-root-path": "^3.1.0",
    "archiver": "^5.3.1",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "babel-preset-minify": "^0.5.2",
    "body-parser": "^1.20.0",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "colors": "^1.4.0",
    "commander": "^9.4.0",
    "concurrently": "^7.4.0",
    "cookie-parser": "^1.4.6",
    "css-loader": "^6.7.1",
    "directory-tree": "^3.3.1",
    "express": "^4.18.1",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "inquirer": "^8.2.4",
    "install": "^0.13.0",
    "js-cookie": "^3.0.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "message-format-inline": "^1.0.4",
    "mini-css-extract-plugin": "^2.6.1",
    "minimist": "^1.2.6",
    "moment": "^2.29.4",
    "node-fetch": "^3.2.10",
    "nodemon": "^2.0.20",
    "nunjucks": "^3.2.3",
    "nunjucks-intl": "^1.0.3",
    "object-assign-deep": "^0.4.0",
